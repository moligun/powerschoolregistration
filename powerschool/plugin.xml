        name="LSC Registration"
        version="1.03"
        description="Custom LSC Student Registration">
	<oauth />
    <openid host="34-48552.lsc.k12.in.us" port="3000">
        <links>
            <link title="LSC Teacher Portal" display-text="newApp" path="/auth/login">
